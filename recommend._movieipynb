    "def convert2(obj):\n",
    "   M=[] \n",
    "   count=0\n",
    "   for i in ast.literal_eval(obj):\n",
    "        if(count!=3):\n",
    "          M.append(i['name'])\n",
    "          count+=1\n",
    "        else: \n",
    "            break\n",
    "   return M    "
   ]
